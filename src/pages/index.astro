---
// layout imports
import MainLayout from "../layouts/MainLayout.astro"

// component imports
import Gradient from "../components/Gradient"
---

<MainLayout bodyClasses="!bg-transparent">
    <Gradient client:load/>

    <section class="gap-5">
        <img src="data:image/webp;base64,"
             alt="profile picture of the developer of this site"
             class="w-20 aspect-square rounded-full">
        <h1 class="text-4xl">I make random shit</h1>
        <p>will get prettier soon</p>
        <ul>
            <li>
                <a class="link link-primary" href="/tools/">Tools</a>
            </li>
        </ul>
    </section>
</MainLayout>